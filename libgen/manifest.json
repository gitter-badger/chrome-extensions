{
   "content_scripts": [ {
      "js": [ "js/jquery/jquery.min.js", "src/inject/inject.js" ],
      "matches": [ "http://www.amazon.com/*", "https://www.amazon.com/*" ]
   } ],
   "default_locale": "en",
   "description": "Searches and displays ebook results from libgen.io (Library Genesis) when you visit a book page on Amazon",
   "icons": {
      "128": "icons/icon128.png",
      "48": "icons/icon48.png"
   },
   "manifest_version": 2,
   "name": "LibGen",
   "permissions": [ "http://libgen.io/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0",
   "web_accessible_resources": [ "images/*", "js/jquery/jquery.min.map" ]
}
